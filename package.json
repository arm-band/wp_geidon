{
    "name": "geidon",
    "version": "0.0.4",
    "description": "wp-env を使用した最小限の開発環境",
    "main": "gulpfile.js",
    "scripts": {
        "wp-env": "wp-env",
        "wp:start": "wp-env start",
        "wp:stop": "wp-env stop",
        "gulp": "gulp",
        "start": "npm-run-all -s setting_env wp:start gulp",
        "restart": "npm-run-all -s wp:stop wp:start gulp",
        "setting_env": "node ./bin/tasks/settingenv"
    },
    "author": "Arm Band",
    "license": "ISC",
    "devDependencies": {
        "@wordpress/env": "^4.1.3",
        "browser-sync": "^2.27.7",
        "dotenv": "^10.0.0",
        "gulp": "^4.0.2",
        "gulp-imagemin": "^7.1.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-svgo": "^9.0.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.0.0",
        "sass": "^1.43.4",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "webpack": "^5.61.0",
        "webpack-stream": "^7.0.0",
        "terser-webpack-plugin": "^5.2.4",
        "clean-webpack-plugin": "^4.0.0",
        "npm-run-all": "^4.1.5"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=6.14.9"
    },
    "engineStrict": true
}
